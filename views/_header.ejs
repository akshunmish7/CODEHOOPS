<header>
    <div id="part1">
        <h1>codeial</h1>
    </div>
    <ul id="part2">
        <% if (locals.user){%>
            <li>
                <a href="/users/profile/<%= locals.user.id %>" id="logged-user">
                    <img id="user-img" src="<%= locals.user.avatar %>" alt="" width="50">
                    <span id="user-name"> <%= user.name %> </span>
                </a>
            </li>
            <li>
                <a href="/users/sign-out">Sign Out</a>
            </li>
        <% }else{ %>
            <li>
                <a href="/users/sign-in">Sign In </a>
            </li>
            <li>
                <a href="/users/sign-up">Sign Up </a>
            </li>
        <% } %>
    </ul>
    
</header>